<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.rawanalduhyshi.productwithrecycleviewwithintent.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <activity
        android:id="@+id/phoneActivity"
        android:name="com.rawanalduhyshi.productwithrecycleviewwithintent.PhoneActivity"
        android:label="activity_phone"
        tools:layout="@layout/activity_phone" >
        <argument
            android:name="name"
            app:argType="string" />
        <argument
            android:name="price"
            app:argType="string" />
    </activity>
</navigation>